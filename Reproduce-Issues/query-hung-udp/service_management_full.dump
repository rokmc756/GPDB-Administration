    LANGUAGE plpgsql SECURITY DEFINER NO SQL
    AS $_$

DECLARE
   -- Declare function variables
   -- ###########################

   l_session_num ALIAS FOR $1;
   l_folder_nm ALIAS FOR $2;
   l_wf_run_id ALIAS FOR $3;
   l_wf_nm ALIAS FOR $4;
